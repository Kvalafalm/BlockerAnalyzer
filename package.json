{
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "concurrently": "^7.1.0",
    "config": "^3.3.7",
    "cross-env": "^7.0.3",
    "express": "^4.17.3",
    "express-validator": "^6.14.0",
    "moment": "^2.29.3",
    "mongoose": "^6.2.9",
    "node-fetch": "^3.2.3",
    "nodemon": "^2.0.15",
    "path": "^0.12.7",
    "ts-node": "^10.8.0"
  },
  "type": "module",
  "devDependencies": {
    "@types/config": "^0.0.41",
    "@types/express": "^4.17.13",
    "@types/node": "^17.0.23",
    "typescript": "^4.7.2"
  },
  "scripts": {
    "start": "cross-env NODE_ENV=production node app.js",
    "serverTS": "node --loader ts-node/esm app.ts",
    "serverDTS": "nodemon app.ts",
    "server": "nodemon app.js",
    "client": "npm run start --prefix client",
    "client:install": "npm install --prefix client",
    "client:build": "npm run build --prefix client",
    "dev": "cross-env NODE_ENV=development concurrently \"npm run serverTS\" \"npm run client\""
  }
}
